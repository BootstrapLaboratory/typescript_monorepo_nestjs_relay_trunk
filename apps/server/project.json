{
    "name": "server",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build",
                "cwd": "apps/server"
            }
        },
        "start:dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run start:dev",
                "cwd": "apps/server"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run lint",
                "cwd": "apps/server"
            }
        }
    }
}
